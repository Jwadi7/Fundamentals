#include <vector>
#include <string>
using namespace std;

vector<string> friendOrFoe(const vector<string>& input) {
    vector <string> friends = {};
    for(int i = 0; i<input.size(); i++)
    {
        if(input.at(i).length() == 4)
        {
            friends.push_back(input.at(i));
        }
        else 
        {
            continue;
        }
    }
    return friends;
}
