#include <string>

using namespace std;

int getCount(const string& inputStr){
  int num_vowels = 0;
  for (int i =0; i<inputStr.length(); i++)
  {
    switch(inputStr[i]){
        case 'a':
        num_vowels ++;
        continue;
        case 'e':
        num_vowels++;
        continue;
        case 'o':
        num_vowels++;
        continue;
        case 'u':
        num_vowels++;
        continue;
        case 'i':
        num_vowels++;
        continue;
    }
  }
  return num_vowels;
}
